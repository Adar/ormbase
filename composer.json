{
  "name": "ecsco/ormbase",
  "type": "library",
  "homepage": "http://e-cs.co",
  "time": "2015-01-06",
  "description": "ORM Basic Libraries",
  "license": "proprietary",
  "authors": [
    {
      "name": "Christian Senkowski",
      "email": "cs@e-cs.co"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "ecsco\\ormbase\\": "src/"
    }
  },
  "require": {
    "php": ">=7.0",
    "phpmd/phpmd": "2.1.3"
  },
  "suggest": {
    "ext-memcached": "Allows you to cache entities when implementing iCachable"
  },
  "require-dev": {
    "phpunit/phpunit": "5.1.*"
  },
  "config": {
    "process-timeout": 300,
    "use-include-path": false
  }
}
